{
  "name": "signalement-app",
  "version": "0.0.1-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start:local": "npm run generate-json-files && npm run generate-blog-metadata && ng serve --host 0.0.0.0",
    "start": "node dist/server.js",
    "prebuild": "npm run generate-json-files && npm run generate-blog-metadata && envsub --env API_BASE_URL --env SENTRY_DSN ./src/environments/environment.prod.ts",
    "build": "ng build",
    "test": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run prebuild && npm run build:prod",
    "build:server:prod": "ng run signalement-app:server && webpack --config webpack.server.config.js --progress --colors",
    "build:browser:prod": "ng build --prod",
    "build:prod": "npm run build:server:prod && npm run build:browser:prod",
    "heroku-postbuild": "echo Skip build on Heroku",
    "generate-json-files": "node src/scripts/generate-json-files.ts",
    "generate-blog-metadata": "node src/scripts/generate-blog-metadata.js"
  },
  "private": true,
  "dependencies": {
